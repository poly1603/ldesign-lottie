{
  "name": "@ldesign/lottie",
  "version": "1.0.0",
  "description": "A powerful, feature-rich Lottie animation manager for any framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/adapters/vue/index.d.ts",
      "import": "./dist/adapters/vue/index.js",
      "require": "./dist/adapters/vue/index.cjs"
    },
    "./react": {
      "types": "./dist/adapters/react/index.d.ts",
      "import": "./dist/adapters/react/index.js",
      "require": "./dist/adapters/react/index.cjs"
    },
    "./lit": {
      "types": "./dist/adapters/lit/index.d.ts",
      "import": "./dist/adapters/lit/index.js",
      "require": "./dist/adapters/lit/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "ldesign dev",
    "build": "ldesign build",
    "build:rollup": "rollup -c",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "example:vanilla": "vite examples/vanilla",
    "example:vue": "vite examples/vue",
    "example:react": "vite examples/react",
    "example:lit": "vite examples/lit",
    "example:all": "vite examples",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "lottie",
    "animation",
    "lottie-web",
    "bodymovin",
    "vue",
    "react",
    "framework-agnostic"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "dependencies": {
    "lottie-web": "^5.12.2"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.10.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.56.0",
    "jsdom": "^23.0.1",
    "rollup": "^4.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitepress": "^1.0.0-rc.36",
    "vitest": "^1.0.4",
    "vue": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
