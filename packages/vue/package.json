{
  "name": "@ldesign/lottie-vue",
  "version": "2.0.0",
  "description": "Vue 3 component for Lottie animations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist node_modules"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ldesign/lottie-core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/compiler-sfc": "^3.3.13",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.19.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.4",
    "vue": "^3.3.13",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "lottie",
    "vue",
    "vue3",
    "animation",
    "component"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/lottie",
